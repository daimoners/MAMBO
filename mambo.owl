    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#CartesianCoordinates">
        <rdfs:comment>3D positions defined by orthogonal axes (x,y,z).</rdfs:comment>
        <rdfs:label>CartesianCoordinates</rdfs:label>
        <rdfs:seeAlso>https://en.wikipedia.org/wiki/Cartesian_coordinate_system</rdfs:seeAlso>
        <skos:definition>A coordinate system where particle locations are specified by signed distances from three perpendicular reference planes, typically in Ångströms or nanometers.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Particle">
        <rdfs:comment>A localized material unit with emergent properties.</rdfs:comment>
        <rdfs:label>Particle</rdfs:label>
        <rdfs:seeAlso>https://goldbook.iupac.org/terms/view/L03457</rdfs:seeAlso>
        <rdfs:seeAlso>https://goldbook.iupac.org/terms/view/N04237</rdfs:seeAlso>
        <rdfs:seeAlso>https://goldbook.iupac.org/terms/view/S05713</rdfs:seeAlso>
        <skos:definition>A discrete physical entity (e.g., colloidal particle, coarse-grained bead) that may represent atomic clusters or simplified molecular groups in multiscale models.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#SphericalCoordinates">
        <rdfs:comment>Positions defined by radial distance and angles.</rdfs:comment>
        <rdfs:label>SphericalCoordinates</rdfs:label>
        <rdfs:seeAlso>https://en.wikipedia.org/wiki/Spherical_coordinate_system</rdfs:seeAlso>
        <skos:definition>A coordinate system where particle locations are specified by radial distance (r) from a fixed origin, polar angle (θ) from zenith, and azimuthal angle (φ) from a reference axis.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_barostat">
        <rdfs:label>has_barostat</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_center_of_mass_coordinates">
        <rdfs:label>has_center_of_mass_coordinates</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_coordinates">
        <rdfs:comment>Associates atoms with their spatial position representations.</rdfs:comment>
        <rdfs:label>has_coordinates</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.C01333</rdfs:seeAlso>
        <skos:definition>Connects an atom to its mathematical description of location in 3D space, which may be specified in Cartesian (x,y,z) or spherical (r,θ,φ) coordinate systems.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_particle">
        <rdfs:label>has_particle</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_structural_particle">
        <rdfs:comment>Identifies coarse-grained or composite units in structures.</rdfs:comment>
        <rdfs:label>has_structural_particle</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.P04455</rdfs:seeAlso>
        <skos:definition>Links a material structure to its constituent particles that may represent simplified representations of atomic groups or discrete nanoscale components in multiscale models.</skos:definition>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_thermostat">
        <rdfs:label>has_thermostat</rdfs:label>
    </rdf:Description>
    <rdf:Description rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_atom_of">
        <rdfs:comment>Inverse of has_atom - indicates molecular containment.</rdfs:comment>
        <rdfs:label>is_atom_of</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00493</rdfs:seeAlso>
        <skos:definition>Specifies that an atom is structurally or chemically incorporated within a larger molecular entity (functional group, system, or particle).</skos:definition>
    </rdf:Description>
    


