        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Indicates atomic participation in material structures.</rdfs:comment>
        <rdfs:label>is_structural_atom_of</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00493</rdfs:seeAlso>
        <skos:definition>Specifies that an atom is structurally incorporated within a defined material architecture, with associated positional and chemical role information.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_entity_of -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_entity_of">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#StructuralEntity"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>General inverse of has_structural_entity.</rdfs:comment>
        <rdfs:label>is_structural_entity_of</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.S06060</rdfs:seeAlso>
        <skos:definition>Indicates that a structural component (atom, molecule, particle) belongs to a larger material structure description at any organizational scale.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_functional_group_of -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_functional_group_of">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#FunctionalGroup"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Indicates functional group participation in structures.</rdfs:comment>
        <rdfs:label>is_structural_functional_group_of</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.F02555</rdfs:seeAlso>
        <skos:definition>Specifies that a functional group is formally recognized as part of a material&apos;s structural description, with defined positional and chemical role information.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_molecular_system_of -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_molecular_system_of">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Indicates molecular system participation in structures.</rdfs:comment>
        <rdfs:label>is_structural_molecular_system_of</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.M03987</rdfs:seeAlso>
        <skos:definition>Specifies that a molecular system is formally incorporated into a material&apos;s structural description as a discrete organizational unit.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_particle_of -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_structural_particle_of">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Particle"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Indicates particle participation in structures.</rdfs:comment>
        <rdfs:label>is_structural_particle_of</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.P04455</rdfs:seeAlso>
        <skos:definition>Specifies that a particle representation is formally included in a material&apos;s structural description, typically in coarse-grained or mesoscale models.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#predicts -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#predicts">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Simulation"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Links simulations to their forecasted material characteristics.</rdfs:comment>
        <rdfs:label>predicts</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1016/j.cpc.2021.108171</rdfs:seeAlso>
        <skos:definition>Associates a computational simulation with the molecular structures or material properties it theoretically anticipates based on modeled physical principles.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#second_atom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#second_atom">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Angle"/>
