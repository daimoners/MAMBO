     -->

    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#characterizes -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#characterizes">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Experiment"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Links an experimental investigation to the specific structural or material property features it examines.</rdfs:comment>
        <rdfs:label>characterizes</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.C01020</rdfs:seeAlso>
        <skos:definition>Associates an experimental procedure with the molecular-level characteristics (structural arrangements or measurable properties) it is designed to investigate or quantify.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#describes_molecular_material_property -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#describes_molecular_material_property">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Result"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:comment>Associates a computational or experimental result with the material property it quantifies.</rdfs:comment>
        <rdfs:label>describes_molecular_material_property</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.P04891</rdfs:seeAlso>
        <skos:definition>Links output data from simulations or measurements to the specific molecular material characteristic (e.g., conductivity, thermal stability) that the data represents.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#describes_structure -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#describes_structure">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Result"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Connects result data to the molecular or crystalline structure it characterizes.</rdfs:comment>
        <rdfs:label>describes_structure</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.S06060</rdfs:seeAlso>
        <skos:definition>Associates output from structural determination methods (e.g., diffraction, microscopy) with the atomic/molecular arrangement they describe, including spatial coordinates and connectivity.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#first_atom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#first_atom">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Angle"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Bond"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Dihedral"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:comment>Identifies the initial atom in a bonded interaction sequence.</rdfs:comment>
        <rdfs:label>first_atom</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00493</rdfs:seeAlso>
        <skos:definition>Specifies the first participating atom in a geometric relationship (bond, angle, or dihedral), following IUPAC convention for ordered specification of molecular connectivity.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#fourth_atom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#fourth_atom">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Dihedral"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:comment>Designates the terminal atom in a four-atom dihedral angle.</rdfs:comment>
        <rdfs:label>fourth_atom</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1002/jcc.23422</rdfs:seeAlso>
        <skos:definition>Identifies the fourth atom (D) in a dihedral angle defined by atoms A-B-C-D, which determines the torsional rotation about the central B-C bond.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_aggregation_type -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_aggregation_type">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularAggregate"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#AggregationType"/>
        <rdfs:comment>Specifies the self-assembly pattern of molecular systems into larger structures.</rdfs:comment>
        <rdfs:label>has_aggregation_type</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00273</rdfs:seeAlso>
        <skos:definition>Associates a molecular aggregate with its classification of non-covalent organization (e.g., crystalline, micellar, amorphous) that emerges from intermolecular interactions.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_angle -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_angle">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#FunctionalGroup"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Angle"/>
        <rdfs:comment>Associates molecular entities with their constituent bond angles.</rdfs:comment>
        <rdfs:label>has_angle</rdfs:label>
        <rdfs:seeAlso>http://purl.obolibrary.org/obo/MOP_0000142</rdfs:seeAlso>
        <skos:definition>Links a molecular system or functional group to the three-atom angle measurements that define its geometric conformation and local strain energy contributions.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_atom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_atom">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_atom_of"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#FunctionalGroup"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularAggregate"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Particle"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:comment>Indicates atomic constituents of molecular entities.</rdfs:comment>
        <rdfs:label>has_atom</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00493</rdfs:seeAlso>
        <skos:definition>Specifies which atoms are structurally or chemically incorporated within a molecular system, functional group, or particle representation.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_barostat -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_barostat">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Algorithm"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#BarostatCoupling"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_bond -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_bond">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#FunctionalGroup"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Bond"/>
        <rdfs:comment>Associates molecular entities with their covalent connections.</rdfs:comment>
        <rdfs:label>has_bond</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.B00705</rdfs:seeAlso>
        <skos:definition>Links a molecular system or functional group to the chemical bonds between its constituent atoms that define connectivity and molecular graph topology.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_center_of_mass_coordinates -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_center_of_mass_coordinates">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#CartesianCoordinates"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#SphericalCoordinates"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_component -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_component">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#BondedPotentialTerm"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#NonBondedPotentialTerm"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#ForceField"/>
        <rdfs:comment>Identifies constituent potential terms in a force field.</rdfs:comment>
        <rdfs:label>has_component</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1002/jcc.20065</rdfs:seeAlso>
        <skos:definition>Associates a bonded or non-bonded potential energy term with the force field model that incorporates it as part of its total energy function.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_composition -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_composition">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterial"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Composition"/>
        <rdfs:comment>Specifies the elemental or molecular makeup of a material.</rdfs:comment>
        <rdfs:label>has_composition</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.C01203</rdfs:seeAlso>
        <skos:definition>Links a molecular material to its quantitative description of chemical constituents, including stoichiometry and relative proportions of components.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_computational_input -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_computational_input">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Simulation"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Associates simulations with their required structural or property data inputs.</rdfs:comment>
        <rdfs:label>has_computational_input</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1002/jcc.20291</rdfs:seeAlso>
        <skos:definition>Connects a computational simulation to the initial molecular structures or material property values that serve as starting conditions for modeling.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_computational_output -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_computational_output">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_computed_by"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Simulation"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Links simulations to their generated structural or property data results.</rdfs:comment>
        <rdfs:label>has_computational_output</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1016/j.softx.2019.100434</rdfs:seeAlso>
        <skos:definition>Associates a computational simulation with the predicted molecular structures or material properties that constitute its primary output data.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_coordinates -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_coordinates">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#CartesianCoordinates"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#SphericalCoordinates"/>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_dihedral -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_dihedral">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#FunctionalGroup"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Dihedral"/>
        <rdfs:comment>Identifies torsional angles within molecular entities.</rdfs:comment>
        <rdfs:label>has_dihedral</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1021/ja00231a008</rdfs:seeAlso>
        <skos:definition>Links a molecular system or functional group to the four-atom dihedral angles that describe its conformational flexibility and rotational barriers.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_ensemble -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_ensemble">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularDynamics"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#SimulationEnsemble"/>
        <rdfs:comment>Specifies the thermodynamic constraints applied in molecular dynamics.</rdfs:comment>
        <rdfs:label>has_ensemble</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1063/1.1699114</rdfs:seeAlso>
        <skos:definition>Associates a molecular dynamics simulation with its governing thermodynamic ensemble (e.g., NVT, NPT) that defines the conserved quantities and system behavior.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_experimental_input -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_experimental_input">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Experiment"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Links experiments to their required starting materials or reference data.</rdfs:comment>
        <rdfs:label>has_experimental_input</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.E02271</rdfs:seeAlso>
        <skos:definition>Connects an experimental procedure to the molecular structures or baseline property measurements that serve as its initial conditions or calibration standards.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_experimental_output -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_experimental_output">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_measured_by"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Experiment"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularMaterialProperty"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Structure"/>
        <rdfs:comment>Associates experiments with their measured structural or property results.</rdfs:comment>
        <rdfs:label>has_experimental_output</rdfs:label>
        <rdfs:seeAlso>http://purl.obolibrary.org/obo/IAO_0000109</rdfs:seeAlso>
        <skos:definition>Links an experimental investigation to the empirical data it produces about molecular structures or material characteristics under defined measurement conditions.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_functional_group -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_functional_group">
        <owl:inverseOf rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#is_functional_group_of"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularAggregate"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularSystem"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#FunctionalGroup"/>
        <rdfs:comment>Identifies chemically reactive subunits within molecular systems.</rdfs:comment>
        <rdfs:label>has_functional_group</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.F02555</rdfs:seeAlso>
        <skos:definition>Associates a molecular entity with its constituent functional groups that confer characteristic reactivity patterns and influence material properties.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_integrated_atom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_integrated_atom">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#ExternalCoupling"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:comment>Specifies atoms coupled to external thermodynamic reservoirs.</rdfs:comment>
        <rdfs:label>has_integrated_atom</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1063/1.448118</rdfs:seeAlso>
        <skos:definition>Connects an external coupling algorithm (thermostat/barostat) to the specific atoms whose motion is directly regulated to maintain system temperature or pressure.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_integrator -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#has_integrator">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#MolecularDynamics"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Integrator"/>
        <rdfs:comment>Associates molecular dynamics methods with their numerical integration algorithms.</rdfs:comment>
        <rdfs:label>has_integrator</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1103/PhysRev.159.98</rdfs:seeAlso>
