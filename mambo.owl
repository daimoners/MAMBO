        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:comment>Identifies the central atom in three-atom angles or terminal atom in bonds.</rdfs:comment>
        <rdfs:label>second_atom</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00493</rdfs:seeAlso>
        <skos:definition>Specifies the second participating atom in geometric relationships: the central atom (B) in angles (A-B-C) or the second atom (B) in bonds (A-B).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#third_atom -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#third_atom">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Angle"/>
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Dihedral"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Atom"/>
        <rdfs:comment>Identifies the terminal atom in three-atom angles.</rdfs:comment>
        <rdfs:label>third_atom</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00493</rdfs:seeAlso>
        <skos:definition>Specifies the third atom (C) in angular relationships (A-B-C) that completes the angle measurement about the central atom (B).</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#use_algorithm -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#use_algorithm">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#ComputationalMethod"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Algorithm"/>
        <rdfs:comment>Specifies computational procedures employed by methods.</rdfs:comment>
        <rdfs:label>use_algorithm</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.A00217</rdfs:seeAlso>
        <skos:definition>Links a computational method to the specific algorithms (integration, optimization, sampling) it utilizes to solve mathematical models of molecular systems.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#use_computational_method -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#use_computational_method">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Simulation"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#ComputationalMethod"/>
        <rdfs:comment>Associates simulations with their theoretical frameworks.</rdfs:comment>
        <rdfs:label>use_computational_method</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1351/goldbook.C01201</rdfs:seeAlso>
        <skos:definition>Connects a simulation to the underlying computational approach (e.g., DFT, MD) that defines its physical approximations and numerical techniques.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#use_experimental_method -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#use_experimental_method">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Experiment"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#ExperimentalMethod"/>
        <rdfs:comment>Links experiments to their standardized protocols.</rdfs:comment>
        <rdfs:label>use_experimental_method</rdfs:label>
        <rdfs:seeAlso>http://purl.obolibrary.org/obo/CHMO_0000000</rdfs:seeAlso>
        <skos:definition>Associates an experimental investigation with the established measurement technique (spectroscopy, microscopy, etc.) that defines its operational procedures and data generation principles.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#validates -->

    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#validates">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Experiment"/>
        <rdfs:range rdf:resource="https://raw.githubusercontent.com/daimoners/MAMBO/master/mambo.owl#Simulation"/>
        <rdfs:comment>Establishes confirmation relationships between experiments and simulations.</rdfs:comment>
        <rdfs:comment>Links an experimental result to a simulation it confirms.</rdfs:comment>
        <rdfs:label>validates</rdfs:label>
        <rdfs:seeAlso>https://doi.org/10.1002/jcc.23422</rdfs:seeAlso>
        <skos:definition>Links an experimental result to computational predictions it empirically confirms, establishing the theoretical model&apos;s reliability for specific material systems or conditions.</skos:definition>
    </owl:ObjectProperty>
    


